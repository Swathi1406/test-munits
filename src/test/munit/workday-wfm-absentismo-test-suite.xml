				</munit-tools:after-call>
			</munit-tools:spy>
			<munit-tools:mock-when processor="sftp:read" doc:name="Mock SFTP Read input file">
      <munit-tools:with-attributes>
        <munit-tools:with-attribute attributeName="doc:name" whereValue="SFTP Read input file"/>
      </munit-tools:with-attributes>
      <munit-tools:then-return>
        <munit-tools:payload value="#[MunitTools::getResourceAsString('examples/sftpInputRequest.txt')]"
                             mediaType="text/plain"/>
      </munit-tools:then-return>
    </munit-tools:mock-when>
    <munit-tools:spy processor="sftp:read" doc:name="Spy SFTP â€“ set varOriginalFilePayload">
      <munit-tools:after-call>
        <set-variable variableName="varOriginalFilePayload"
                      value="#[MunitTools::getResourceAsString('examples/sftpInputRequest.txt')]"/>
      </munit-tools:after-call>
    </munit-tools:spy>
			<!-- [STUDIO:"Mock varMapping"]<munit-tools:mock-when processor="set-variable" doc:name="Mock varMapping">
                <munit-tools:with-attributes>
                    <munit-tools:with-attribute attributeName="doc:name" whereValue="varMapping" />
                </munit-tools:with-attributes>
                <munit-tools:then-return />
            </munit-tools:mock-when> [STUDIO] -->
